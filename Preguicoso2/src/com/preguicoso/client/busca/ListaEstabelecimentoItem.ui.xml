<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		
			<div class ="restaurantListIndividual">
				<div class="grid_3 omega alpha">
					<g:Image ui:field="logo"/>
				</div>
				<div class="grid_6 alpha omega">
					<div class="grid_4">
						<h3><g:Label ui:field="nome"/></h3>
					</div>
					<div class="grid_2 omega alpha">
						<g:InlineHyperlink text="Delivery Online" targetHistoryToken="newHistoryToken" styleName="estabelecimentoDeliveryButton" ui:field="link"/>
					</div>
					<div class="clear">
					</div>
					<div class="grid_6 alpha omega descricaoLista">
						<small></small>
						<g:Label ui:field="descricao"/>
					</div>
					<div class="grid_6 alpha omega enderecoLista">
						<small></small>
						<g:Label ui:field="endereco"/>
					</div>
					
						Status:
						<g:Label ui:field="status"/>
					
				</div>
			</div>
			
			</g:HTMLPanel>
</ui:UiBinder> 