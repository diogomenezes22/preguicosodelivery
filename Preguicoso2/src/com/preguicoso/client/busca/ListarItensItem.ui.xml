<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<div class ="itemListIndividual">
				<div>
					<g:Image ui:field="logo"/>
				</div>
				<div >
					<div>
						<h3><g:Label ui:field="nome"/></h3>
					</div>
					<div class="precoListItem">
						<small>R$</small>
						<g:InlineLabel ui:field="descricao"/>
					</div>
					<div>
						<g:InlineHyperlink text="Entrar na Loja" targetHistoryToken="newHistoryToken" styleName="estabelecimentoDeliveryButton" ui:field="link"/>
					</div>
					
					<div class="enderecoLista">
						<small></small>
						<g:Label ui:field="endereco"/>
					</div>
					
				</div>
			</div>
	</g:HTMLPanel>
</ui:UiBinder> 