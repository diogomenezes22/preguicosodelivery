<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<table border='0' class='endereco'>
			<tr>
				<th COLSPAN="3">
					<g:TextBox ui:field="nome" text="Nome Completo" />
					<g:TextBox ui:field="email" text="E-mail" />
					<g:PasswordTextBox ui:field="senha" text="Senha" />
					<g:PasswordTextBox ui:field="senhaCheck" text="Senha" />
					<g:TextBox ui:field="telefone" text="Telefone Residencial" />
					<g:TextBox ui:field="celular" text="Celular" />
				</th>
			</tr>
		</table>
		<g:HTMLPanel ui:field="antesDoCep">
			<table border='0' class='endereco'>
				<tr>
					<th COLSPAN="3">
						<g:TextBox ui:field="enderecoCep" text="CEP" />
						<g:Button ui:field="verificarCep" text="Verificar CEP" />
						<g:Button ui:field="pularCep" text="Não usar CEP" />
					</th>
				</tr>
			</table>
		</g:HTMLPanel>
		<g:HTMLPanel ui:field="depoisDoCep">
			<table border='0' class='endereco'>
				<tr>
					<th COLSPAN="3">
						<g:TextBox ui:field="endereco_rua" width="355px" text="Logradouro" />
					</th>
					<td width='50'>
						<g:TextBox ui:field="endereco_numero" width="100px"
							text="Número" />
					</td>
				</tr>
				<tr class='legenda_endereco'>
					<th COLSPAN="4">Informe o nome da avenida, rua, praça,
						travessa, etc.</th>
				</tr>
				<tr>
					<th COLSPAN="2">
						<g:TextBox ui:field="endereco_complemento" text="Complemento" />
					</th>
					<th COLSPAN="2">
						<g:ListBox ui:field="endereco_bairro" />
					</th>
				</tr>
				<tr class='legenda_endereco'>
					<th COLSPAN="4">Exemplo: Apto. 31.</th>
				</tr>
			</table>
			<tr>
				<th COLSPAN="2">
					<g:Button ui:field="pedir">Pedir</g:Button>
				</th>
			</tr>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 