<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:FocusPanel styleName="restaurante_cardapio_item" ui:field="focusPanel">
		<g:HTMLPanel>
				<g:Image ui:field="imagem"/>
				<div class="nome">
					<g:Label text="" ui:field="prato"/>
				</div>
				<div class="">
					<g:Label text="" ui:field="descricao"/>
				</div>
				<div class="preco">
					R$ <g:InlineLabel text="" ui:field="preco" styleName="preco"/>
				</div>
				
				<div class="">
					<a class="estabelecimentoDeliveryButton">Comprar</a>
					<g:HTMLPanel ui:field="botaoAdd"/>
				</div>
				
		</g:HTMLPanel>
	</g:FocusPanel>
</ui:UiBinder> 