<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
		.searchFieldDiv {
			width: 100%;
			height: 47px;
			margin-bottom: 12px;
		}
	</ui:style>
	<g:HTMLPanel styleName="{style.searchFieldDiv}">
		<g:SuggestBox ui:field="suggestBox" styleName="gwt-SearchField"/>
	</g:HTMLPanel>
</ui:UiBinder> 