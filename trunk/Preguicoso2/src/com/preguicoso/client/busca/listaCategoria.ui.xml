<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel styleName="grid_12">
		<g:HTMLPanel ui:field="localPanel" styleName="categoria">
			<h4>Descubra quais parceiros entregam no seu bairro:</h4>
			<g:ListBox ui:field="cidadeBox" />
			<g:ListBox ui:field="bairroBox" />
		</g:HTMLPanel>
		<g:InlineHyperlink text="Trocar o Bairro de entrega" targetHistoryToken="index" ui:field="todas"
			styleName="categoria" />
		<g:HTMLPanel ui:field="categoriasPanel"></g:HTMLPanel>

	</g:HTMLPanel>
</ui:UiBinder> 